                        zoneDefectCountProvider[singleZone][`${defect}`] = data.data[group][category][defect][subDefect][bodyNumber][singleZone]
                      }else{
                        zoneDefectCountProvider[singleZone][`${defect}`] += data.data[group][category][defect][subDefect][bodyNumber][singleZone]
